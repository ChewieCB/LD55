GameManager="*res://src/utils/singletons/GameManager.tscn"
SoundManager="*res://addons/sound_manager/sound_manager.gd"
BuildThumbnail="*res://src/utils/singletons/build_thumbnail.gd"

[display]

window/size/viewport_width=1366
window/size/viewport_height=768
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/godot_state_charts/plugin.cfg", "res://addons/sound_manager/plugin.cfg")

